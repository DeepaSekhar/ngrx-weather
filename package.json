{
  "name": "ngrx-weather",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-environment": "cp src/environments/local_environment.ts src/environments/environment.ts",
    "build-local": "npm run copy-environment && npm run build",
    "serve-local": "npm run copy-environment && ng serve",
    "coverage-report": "ng test --code-coverage --watch=true",
    "delete-coverage": "rm -rf coverage",
    "code-coverage": "npm run delete-coverage && npm run coverage-report",
    "build-prod": "ng build --prod",
    "test-prod": "ng test --watch=false",
    "firebase-deploy": "firebase deploy",
    "environment-variables": "python scripts/environment_variables.py $OPEN_WEATHER_MAP_API_KEY"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~10.1.2",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/schematics": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@ngxd/core": "^7.0.1",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "puppeteer": "^1.12.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/language-service": "~10.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
